name: xcodebuild iphone 12

on:
  push:
    branches: [ main ]
  pull_request:
    branches: [ main ]

jobs:
  build:

    runs-on: macos-latest

    steps:
    - uses: actions/checkout@v2
    - name: Run tests
      run: xcodebuild test-without-building -scheme 'KingsCup (iOS)' -destination 'platform=iOS Simulator,name=iPhone 12'
